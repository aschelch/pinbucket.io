{
    "name": "PinBucket.io",
    "version": "1.0",
    "description": "Share tech articles with your teammates.",
    "manifest_version": 2,
    "permissions": [
        "tabs", "storage", "https://www.pinbucket.io/*"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon16.png",
            "32": "images/icon32.png",
            "48": "images/icon48.png",
            "128": "images/icon128.png"
        }
    },
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    }
  }